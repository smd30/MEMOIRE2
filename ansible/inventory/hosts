[production]
kds-prod-01 ansible_host=your-production-server.com ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/kds_prod_key

[staging]
kds-staging-01 ansible_host=your-staging-server.com ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/kds_staging_key

[all:vars]
ansible_python_interpreter=/usr/bin/python3
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
